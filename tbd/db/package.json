    "remeda": "1.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@openint/vdk": "workspace:*",
    "@types/json-stable-stringify": "^1.1.0",
    "prettier": "3.1.0",
    "prettier-plugin-sql": "0.18.1"
  }
