{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": [
    "CHROMATIC_PROJECT_TOKEN",
    "CLERK_SECRET_KEY",
    "DATABASE_URL",
    "DOCKER_POSTGRES_URL",
    "DOCKER_SUPABASE_URL",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "INTEGRATION_TEST_API_KEY",
    "INTEGRATION_TEST_SECRET",
    "JWT_SECRET",
    "NANGO_SECRET_KEY",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_CLERK_SUPABASE_JWT_TEMPLATE_NAME",
    "NEXT_PUBLIC_COMMANDBAR_ORG_ID",
    "NEXT_PUBLIC_NANGO_PUBLIC_KEY",
    "NEXT_PUBLIC_POSTHOG_WRITEKEY",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SENTRY_ORG",
    "POSTGRES_URL",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_CRON_MONITOR_URL",
    "SENTRY_IGNORE_API_RESOLUTION_ERROR",
    "SENTRY_ORG",
    "STARBASE_AUTH_TOKEN",
    "STARBASE_ENDPOINT",
    "ccfg_hubspot_CLIENT_ID",
    "ccfg_hubspot_CLIENT_SECRET",
    "ccfg_hubspot__SCOPE",
    "ccfg_plaid__CLIENT_ID",
    "ccfg_plaid__CLIENT_SECRET_PRODUCTION",
    "ccfg_plaid__CLIENT_SECRET_SANDBOX",
    "ccfg_qbo_CLIENT_ID",
    "ccfg_qbo_CLIENT_SECRET",
    "ccfg_qbo_SCOPE",
    "conn_greenhouse_API_KEY",
    "int_discord__CLIENT_ID",
    "int_discord__CLIENT_SECRET",
    "int_heron__apiKey",
    "int_hubspot__CLIENT_ID_OLD",
    "int_hubspot__CLIENT_SECRET_OLD",
    "int_merge__apiKey",
    "int_plaid__CLIENT_ID",
    "int_plaid__CLIENT_SECRET_DEVELOPMENT",
    "int_plaid__CLIENT_SECRET_PRODUCTION",
    "int_plaid__CLIENT_SECRET_SANDBOX"
  ],
  "tasks": {
    "download": {
      "outputs": ["*.oas.json"]
    },
    "generate": {
      "dependsOn": ["^generate", "download"],
      "cache": false,
      "outputs": ["*.oas.d.ts"]
    },
    "clean": {},
    "build": {
      "dependsOn": ["^build", "clean"],
      "cache": false
    },
    "pub": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
